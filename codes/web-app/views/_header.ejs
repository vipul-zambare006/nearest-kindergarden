<!DOCTYPE html>
<html>


<head>

  <meta charset="utf-8">
  <title>Kid-n-Roll : Kindergarten Enrollment Application</title>
  <link rel='stylesheet' href='/stylesheets/style.css' />

  <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCzpsjE7T8mPMHCQ2b-AkJ6N7FKRZf5gGU"></script>
</head>


<body>

  <h1>Kid-n-Roll <small>Kindergarten Enrollment Application</small></h1>


  <ul>
    <% if(currentUser) { %>
    <li>
      <a href="/logout">Log out</a>
    </li>
    <% } else { %>
    <li><a href="/login">Log in</a></li>
    <li><a href="/signup">Sign up</a></li>
    <% } %>
  </ul>


  <% errors.forEach(function(error) { %>
  <div><%= error %></div>
  <% }) %>

  <% infos.forEach(function(info) { %>
  <div><%= info %></div>
  <% }) %>